    <h1>Skate Park</h1>

    <div class="py-5">
      <h2>Iniciar Sesión</h2>
      <hr class="w-50" />

      <form id="form" method="POST">
        <div class="form-group">
          <div class="form-group">
            <label>Email</label>
            <input class="form-control w-50 m-auto" type="email" name="email" placeholder="corre@correo.com"/>
          </div>
          <div class="form-group">
            <label>Password</label>
            <input class="form-control w-50 m-auto" type="password" name="password"/>
            <label id="error"></label>
          </div>
        </div>
        <button class="formulario__btn">Ingresar</button>
        <p>¿Aún no tienes cuenta? <a href="http://localhost:8080/signup">Regístrate</a></p>
      </form>
    </div>

    <script src="assets/js/signIn.js"></script>